    "switch:command"
  ],
  "hediet.vscode-drawio.resizeImages": null,
  "hediet.vscode-drawio.theme": "Kennedy",
  "java.jdt.ls.java.home": "/opt/homebrew/Cellar/openjdk/23.0.1/libexec/openjdk.jdk/Contents/Home",
  "java.jdt.ls.vmargs": "",
  "javascript.updateImportsOnFileMove.enabled": "never",
  "leetcode.defaultLanguage": "typescript",
  "leetcode.workspaceFolder": "/Users/uematsukosuke/Desktop/leetcode",
  "prisma.showPrismaDataPlatformNotification": false,
  "python.createEnvironment.trigger": "off",
  "python.defaultInterpreterPath": "/Users/uematsukosuke/.pyenv/versions/3.12.4/bin/python",
  "redhat.telemetry.enabled": true,
  "terminal.integrated.env.osx": {
    "Q_NEW_SESSION": "1"
  },
  "typescript.preferences.importModuleSpecifier": "non-relative",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "vim.easymotion": true,
  "vim.hlsearch": true,
  "vim.incsearch": true,
  "vim.leader": "<space>",
  "vim.insertModeKeyBindings": [
    {
      "after": [
        "<Esc>"
      ],
      "before": [
        "j",
        "j"
      ]
    }
  ],
  "vim.normalModeKeyBindings": [
    // ファイルツリーの開閉
    {
      "before": [
        "<Leader>",
        "e"
      ],
      "commands": [
        "workbench.action.toggleSidebarVisibility"
      ],
      "when": [
        "editorFocus && vim.mode == 'Normal'"
      ]
    },
    {
      "after": [
        "o",
        "<Esc>"
      ],
      "before": [
        "<leader>",
        "o"
      ]
    },
    {
      "after": [
        "O",
        "<Esc>"
      ],
      "before": [
        "<leader>",
        "O"
      ]
    },
    // エディタ間のカーソル移動(上へ)
    {
      "after": [
        "<C-w>",
        "k"
      ],
      "before": [
        "<C-k>"
      ]
    },
    // エディタ間のカーソル移動(下へ)
    {
      "after": [
        "<C-w>",
        "j"
      ],
      "before": [
        "<C-j>"
      ]
    },
    // エディタ間のカーソル移動(左へ)
    {
      "after": [
        "<C-w>",
        "h"
      ],
      "before": [
        "<C-h>",
      ]
    },
    // エディタ間のカーソル移動(右へ)
    {
      "after": [
        "<C-w>",
        "l"
      ],
      "before": [
        "<C-l>",
      ]
    },
    // エディタの横幅拡大
    {
      "after": [
        "<C-w>",
        ">"
      ],
      "before": [
        "<leader>",
        "."
      ]
    },
    // エディタの横幅減少
    {
      "after": [
        "<C-w>",
        "<"
      ],
      "before": [
        "<leader>",
        ","
      ]
    },
    // エディタの縦幅拡大
    {
      "after": [
        "<C-w>",
        "+"
      ],
      "before": [
        "<leader>",
        ";"
      ]
    },
    // エディタの縦幅縮小
    {
      "after": [
        "<C-w>",
        "-"
      ],
      "before": [
        "<leader>",
        "-"
      ]
    },
    // タブ移動（左へ）
    {
      "after": [
        "g",
        "T"
      ],
      "before": [
        "H"
      ]
    },
    // タブ移動（右へ）
    {
      "after": [
        "g",
        "t"
      ],
      "before": [
        "L"
      ]
    },
    {
      "before": [
        "u"
      ],
      "commands": [
        {
          "command": "undo"
        }
      ]
    },
    {
      "before": [
        "<C-r>"
      ],
      "commands": [
        {
          "command": "redo"
        }
      ]
    },
    // 行末へ移動
    {
      "after": [
        "$"
      ],
      "before": [
        "<leader>",
        "l"
      ]
    },
    // 行頭へ移動(空白無視)
    {
      "after": [
        "^"
      ],
      "before": [
        "<leader>",
        "h"
      ]
    },
    // 対応する括弧へ移動
    {
      "after": [
        "%"
      ],
      "before": [
        "<leader>",
        "m"
      ]
    },
    {
      "after": [
        "a"
      ],
      "before": [
        "あ"
      ]
    },
    {
      "after": [
        "i"
      ],
      "before": [
        "い"
      ]
    },
    {
      "after": [
        "u"
      ],
      "before": [
        "う"
      ]
    },
    {
      "after": [
        "e"
      ],
      "before": [
        "え"
      ]
    },
    {
      "after": [
        "o"
      ],
      "before": [
        "お"
      ]
    },
    // 次の検索結果へ移動し、カーソル行を画面中央にスクロール
    {
      "after": [
        "n",
        "z",
        "z"
      ],
      "before": [
        "n"
      ]
    },
    // 前の検索結果へ移動し、カーソル行を画面中央にスクロール
    {
      "after": [
        "N",
        "z",
        "z"
      ],
      "before": [
        "N"
      ]
    },
    // カーソル下の単語を下方向に検索し、カーソル行を画面中央にスクロール
    {
      "after": [
        "*",
        "z",
        "z"
      ],
      "before": [
        "*"
      ]
    },
    // カーソル下の単語を上方向に検索し、カーソル行を画面中央にスクロール
    {
      "after": [
        "#",
        "z",
        "z"
      ],
      "before": [
        "#"
      ]
    },
    // easy-motionの起動(2文字検索)
    {
      "after": [
        "<leader>",
        "<leader>",
        "2",
        "s"
      ],
      "before": [
        "f"
      ]
    },
    // ファイル検索
    {
      "before": [
        "<leader>",
        "f",
        "f"
      ],
      "commands": [
        "multiCommand.findFiles"
      ],
      "when": [
        "editorFocus && vim.mode == 'Normal'"
      ]
    },
    // 全体検索
    {
      "before": [
        "<leader>",
        "f",
        "w"
      ],
      "commands": [
        "multiCommand.findWithinFiles"
      ],
      "when": [
        "editorFocus && vim.mode == 'Normal'"
      ]
    },
  ],
  "vim.useSystemClipboard": true,
  "vim.visualModeKeyBindings": [
    {
      "after": [
        "<Esc>"
      ],
      "before": [
        "q"
      ]
    },
    {
      "after": [
        "g",
        "b"
      ],
      "before": [
        "<C-k>"
      ]
    },
    {
      "after": [
        "$"
      ],
      "before": [
        "<leader>",
        "l"
      ]
    },
    {
      "after": [
        "^"
      ],
      "before": [
        "<leader>",
        "h"
      ]
    },
    {
      "after": [
        "%"
      ],
      "before": [
        "<leader>",
        "m"
      ]
    }
  ],
  "vim.visualstar": true,
  "find-it-faster.general.showMaximizedTerminal": false, // default
  "multiCommand.commands": [
    {
      "command": "multiCommand.findFiles",
      "interval": 220,
      "sequence": [
        "workbench.action.toggleMaximizedPanel",
        "find-it-faster.findFiles"
